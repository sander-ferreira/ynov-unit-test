    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-jest": "^30.2.0",
    "cypress": "^13.17.0",
    "eslint": "^10.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdoc": "^4.0.5",
    "start-server-and-test": "^2.1.3",
    "vite": "^7.3.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
